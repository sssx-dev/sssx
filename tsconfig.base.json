{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "include": [],
  "exclude": [],
  "ts-node": {
    "esm": true
  },
  "compilerOptions": {
    "composite": true,
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "module": "ESNext",
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "sssx": ["./packages/sssx"],
      "@sssx/eslint-config-sssx": ["./packages/eslint-config-sssx"],
      "@sssx/sitemap-plugin": ["./packages/sitemap-plugin"],
      "@sssx/aws-s3-cloudfront-adapter": ["./packages/aws-s3-cloudfront-adapter"]
    }
  }
}
